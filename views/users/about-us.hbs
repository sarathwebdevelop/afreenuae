<div class="about-area inner-page ">
    <div class="container">
        {{#each abouts}}
        <div class="row">
            <div class="col-lg-6 col-md-12">
                <div class="about-thumb wow animate__slideInLeft">
                    <img src="/images/about-images/{{this._id}}.png" style=" width: 446px;" alt="">
                    <div class="about-shape">
                        <img src="/images/pimages/img6.png" alt="">
                    </div>
                </div>
            </div>
            <div class="col-lg-6 col-md-12 wow animate__slideInRight">
                <div class="section-title">
                    <div class="section-shape">
                        <i class="bi bi-megaphone" style="font-size:40px;"></i>
                    </div>
                    <div class="section-sub-title">
                        <h4>{{this.subtitle}}</h4>
                    </div>
                    <div class="section-main-title about">
                        <h2>{{this.Mheading}}</h2>
                    </div>
                </div>
                <div class="about-content">
                    <div class="about-discribtion">
                        <p>{{this.desc}}</p>

                    </div>


                    <!-- End process bar -->

                    <div class="eulding-btn about col-lg-12 mt-4">
                        <a href="#!">
                            <div class="about-single-box">

                                <div class="about-text">
                                    <h1 class="text-white">18</h1>
                                    <p>YEARS OF EXPERIENCE

                                        .</p>
                                </div>
                            </div>
                        </a>
                    </div>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>
